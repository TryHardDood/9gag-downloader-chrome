{
  "manifest_version": 2,
  "name": "9GAG Downloader",
  "description": "A simple chrome extension to download posts from 9GAG",
  "version": "0.4",
  "permissions": [
    "downloads"
  ],
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.9gag.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
